import entities;

@Path("product")
public class ProductRestful {
	
	@Path("findall")
	@GET
	@Produces(MediaType.APPLICATION_JSON)
	public List<Product> findAll(){
		
		List<Product>  all=new ArrayList<Product>();
		
		all.
	}

}
